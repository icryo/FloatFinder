{
  "name": "FloatFinder",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Helpful filters for CSFloat",
  "icons": {
    "48": "icon48.png"
  },

  
  "permissions": [
    "storage",  
    "activeTab",
    "scripting" 
  ],

  "action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["*://csfloat.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
